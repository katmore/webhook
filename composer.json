{
   "name": "katmore/webhook",
   "description" : "Wrappers and webservice to handle GitHub Webhook requests.",
   "homepage":"https://github.com/katmore/webhook",
    "license": [
       "MIT",
       "GPL-3.0+"
    ],
    "require": {
        "php" : ">=7.2.1",
        "ext-hash" : "*",
        "ext-json" : "*"
    },
    "autoload": {
        "psr-4": { 
            "Webhook\\": "src"
         }
    },
    "autoload-dev": {
        "psr-4": { 
            "Webhook\\TestCase\\": "tests/phpunit/TestCase", 
            "Webhook\\Tests\\": "tests/phpunit/Tests"
         }
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "phpunit/php-code-coverage": "^6.0",
        "php-di/phpdoc-reader": "^2.1"
    }
}
