<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
  <parser>
    <target>.phpdoc-parser</target>
  </parser>
  <transformer>
    <target>docs/phpdoc</target>
  </transformer>
  <transformations>
  		<template name="checkstyle"/>
        <template name="xml"/>
        <template name="clean"/>
  </transformations>
  <files>
    <directory>./src</directory>
  </files>
</phpdoc>